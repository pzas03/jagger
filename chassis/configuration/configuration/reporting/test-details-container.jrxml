<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="TestDetailsContainer" pageWidth="595" pageHeight="842" orientation="Landscape" columnWidth="550"
              leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
    <property name="ireport.zoom" value="1.0"/>
    <property name="ireport.x" value="0"/>
    <property name="ireport.y" value="0"/>
    <template class="java.io.InputStream">
        <![CDATA[$P{context}.getResource("/reporting/styles/default-styles.xml")]]></template>
    <parameter name="context" class="com.griddynamics.jagger.reporting.ReportingContext"/>
    <parameter name="sessionId" class="java.lang.String"/>
    <field name="testName" class="java.lang.String"/>
    <field name="id" class="java.lang.String"/>

    <group name="test" isStartNewPage="false">
        <groupExpression><![CDATA[$F{id}]]></groupExpression>
    </group>
    <title>
        <band height="50" splitType="Prevent">
            <rectangle>
                <reportElement x="0" y="0" width="550" height="40" backcolor="#AAE600"/>
                <graphicElement>
                    <pen lineWidth="0.0" lineStyle="Solid"/>
                </graphicElement>
            </rectangle>
            <staticText>
                <reportElement style="Large_Header_Bold" positionType="Float" x="0" y="0" width="550" height="40"
                               forecolor="#336600"/>
                <textElement textAlignment="Center" verticalAlignment="Middle"/>
                <text><![CDATA[Test Details]]></text>
            </staticText>
        </band>
    </title>
    <detail>
        <band height="90" splitType="Stretch">
            <frame>
                <reportElement x="0" y="0" width="550" height="70"/>
                <rectangle>
                    <reportElement x="0" y="0" width="550" height="15" backcolor="#D7FFB0"/>
                    <graphicElement>
                        <pen lineWidth="0.0" lineStyle="Solid"/>
                    </graphicElement>
                </rectangle>
                <textField isStretchWithOverflow="true">
                    <reportElement style="Header_Bold" x="0" y="0" width="550" height="15" forecolor="#000000"/>
                    <textElement textAlignment="Center" verticalAlignment="Middle"/>
                    <textFieldExpression><![CDATA[$F{testName}]]></textFieldExpression>
                </textField>
                <staticText>
                    <reportElement style="Header_Bold" x="0" y="20" width="500" height="20" forecolor="#000000"/>
                    <textElement textAlignment="Left"/>
                    <text><![CDATA[Validators]]></text>
                </staticText>
                <subreport>
                    <reportElement positionType="Float" x="0" y="45" width="550" height="10"/>
                    <parametersMapExpression><![CDATA[$P{REPORT_PARAMETERS_MAP}]]></parametersMapExpression>
                    <dataSourceExpression>
                        <![CDATA[$P{context}.getMappedProvider("validationResults").getDataSource( $F{id} , $P{sessionId} )]]></dataSourceExpression>
                    <subreportExpression class="net.sf.jasperreports.engine.JasperReport">
                        <![CDATA[(net.sf.jasperreports.engine.JasperReport) $P{context}.getMappedProvider("validationResults").getReport( $F{id} )]]></subreportExpression>
                </subreport>
                <subreport>
                    <reportElement positionType="Float" x="0" y="55" width="550" height="10"/>
                    <parametersMapExpression><![CDATA[$P{REPORT_PARAMETERS_MAP}]]></parametersMapExpression>
                    <dataSourceExpression>
                        <![CDATA[$P{context}.getMappedProvider("metricsResults").getDataSource( $F{id} , $P{sessionId} )]]></dataSourceExpression>
                    <subreportExpression class="net.sf.jasperreports.engine.JasperReport">
                        <![CDATA[(net.sf.jasperreports.engine.JasperReport) $P{context}.getMappedProvider("metricsResults").getReport( $F{id} )]]></subreportExpression>
                </subreport>
                <subreport>
                    <reportElement positionType="Float" x="0" y="65" width="550" height="10"/>
                    <parametersMapExpression><![CDATA[$P{REPORT_PARAMETERS_MAP}]]></parametersMapExpression>
                    <dataSourceExpression>
                        <![CDATA[$P{context}.getMappedProvider("profiler").getDataSource( $F{id} , $P{sessionId} )]]></dataSourceExpression>
                    <subreportExpression class="net.sf.jasperreports.engine.JasperReport">
                        <![CDATA[(net.sf.jasperreports.engine.JasperReport) $P{context}.getMappedProvider("profiler").getReport( $F{id} )]]></subreportExpression>
                </subreport>
            </frame>
        </band>
    </detail>
</jasperReport>
